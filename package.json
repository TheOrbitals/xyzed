{
  "name": "xyzed",
  "version": "0.2.0",
  "description": "A 3-dimensional vector",
  "author": "Don Smith <don@antiantonym.com> (https://blog.dezent.io)",
  "license": "Apache-2.0",
  "homepage": "https://github.com/theorbitals/xyzed",
  "repository": {
    "type": "git",
    "url": "https://github.com/theorbitals/xyzed.git"
  },
  "bugs": {
    "url": "https://github.com/theorbitals/xyzed/issues"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm run lint && tsc",
    "lint": "tslint --project .",
    "test": "jest"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
